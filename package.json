{
  "name": "slack-mcp",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "predev": "echo skip svelte-kit sync",
    "dev": "vite dev --port 5173 --strictPort",
    "build": "vite build",
    "start": "node remote-slack-mcp-server.cjs",
    "post:remote": "node scripts/post-latest-pr-remote.mjs",
    "preview": "svelte-kit preview --port 4173 --host",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "echo skip svelte-kit sync",
    "prebuild": "svelte-kit sync",
    "test": "node simple-test.js",
    "test:vitest": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:simple": "node simple-test.js"
  },
  "dependencies": {
    "@atproto/api": "^0.13.22",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@slack/web-api": "^7.11.0",
    "@supabase/supabase-js": "^2.75.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "openai": "^6.3.0",
    "resend": "^6.1.2",
    "wink-sentiment": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-vercel": "^5.10.3",
    "@sveltejs/kit": "^2.46.5",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "jsdom": "^25.0.1",
    "svelte": "^5.40.0",
    "svelte-check": "^4.3.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^6.3.7",
    "vitest": "^1.6.1"
  }
}
